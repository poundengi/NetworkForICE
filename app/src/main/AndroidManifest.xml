<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chonlavit.networkforice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Group1"/>
        <activity android:name=".Group2"/>
        <activity android:name=".Group3"/>
        <activity android:name=".Group4"/>
        <activity android:name=".Group5"/>
        <activity android:name=".Group6"/>
        <activity android:name=".MainActivity"/>
    </application>

</manifest>
